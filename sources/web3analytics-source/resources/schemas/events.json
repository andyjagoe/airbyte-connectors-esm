      "id": {
        "type": "string"
      },
      "did": {
        "type": "string"
      },
      "updated_at": {
        "type": "integer"
      },
      "anonymousId": {
        "type": "string",
        "title": "anonymousId"
      },
      "event": {
        "type": "string",
        "title": "event"
      },
      "meta": {
        "type": "object",
